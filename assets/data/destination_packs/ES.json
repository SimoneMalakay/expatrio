{
    "country_id": "ES",
    "duration": "more_than_90_days",
    "common_steps": [
        {
            "step_id": "es_enter_id",
            "title": "Enter Spain with Valid ID",
            "deadline": null,
            "authority": "Policía Nacional",
            "requirements": [
                "Valid EU/EEA/CH Passport or National ID card"
            ],
            "outcome": "Legal entry into Spain",
            "dependencies": [],
            "xp": 10,
            "estimated_time_days": 0,
            "notes": null
        },
        {
            "step_id": "es_nie",
            "title": "Obtain NIE (Número de Identidad de Extranjero)",
            "deadline": null,
            "authority": "Policía Nacional / Oficina de Extranjería",
            "requirements": [
                "Passport / ID",
                "Form EX-15",
                "Proof of reason for request",
                "Fee payment (Model 790)"
            ],
            "outcome": "NIE Number Assigned",
            "dependencies": [
                "es_enter_id"
            ],
            "xp": 50,
            "estimated_time_days": 15,
            "notes": "Necessary for all economic and administrative interactions in Spain."
        },
        {
            "step_id": "es_empadronamiento",
            "title": "Address Registration (Empadronamiento)",
            "deadline": null,
            "authority": "Town Hall (Ayuntamiento)",
            "requirements": [
                "Passport / ID",
                "Rental contract or utility bill in your name",
                "Application form"
            ],
            "outcome": "Certificado de Empadronamiento",
            "dependencies": [
                "es_enter_id"
            ],
            "xp": 30,
            "estimated_time_days": 1,
            "notes": "Essential for accessing healthcare and local services."
        },
        {
            "step_id": "es_eu_registration",
            "title": "EU Citizen Registry (Certificado de Registro de Ciudadano de la UE)",
            "deadline": "90 days after arrival",
            "authority": "Oficina de Extranjería / Policía",
            "requirements": [
                "Passport / ID",
                "Form EX-18",
                "Empadronamiento",
                "Proof of status (employment, student, etc.)",
                "Fee payment"
            ],
            "outcome": "Green card/A4 Residence Certificate",
            "dependencies": [
                "es_nie",
                "es_empadronamiento"
            ],
            "xp": 150,
            "estimated_time_days": 30,
            "notes": "This is the actual residence certificate for EU citizens."
        }
    ],
